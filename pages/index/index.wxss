.gudin {
    position: fixed;
    top: 80%;
    right: -216rpx;
    width: 508rpx;
    height: 141rpx;
    z-index: 300;
    font-size: 13px;
    color: #fff;
    padding-left: 100rpx;
    padding-right: 180rpx;
    padding-top: 40rpx;
    box-sizing: border-box;
    text-align: center;
    line-height: 1.5;
    border: 0px;
    outline: none;
    background-color: transparent;
    border-radius: 0px;
}

.gudin::after {
